{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES6",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "outDir": "wwwroot/js",
        "sourceMap": true,
        "lib": [ "ES2017", "DOM" ],
        "jsx": "preserve",
        "strict": true
    },
    "include": [
        "Scripts/**/*"
    ],
    "exclude": [
        "node_modules",
        "wwwroot",
        "**/*.test.ts",
        "**/benchmarks/**"
    ]
}
